	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", "tsconfig.tsbuildinfo"]
		}
	},
	"globalDependencies": [
		".eslintrc.js",
		".eslintignore",
		".lintstagedrc.js",
		".prettierrc.yaml",
		"tsconfig.base.json",
		"tsconfig.build.json",
		"tsconfig.json"
	]
}
